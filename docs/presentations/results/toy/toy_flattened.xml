  <functionDefinition id="bounds__max" name="minimum of arguments">
    <math xmlns="http://www.w3.org/1998/Math/MathML">
      <lambda>
        <bvar>
          <ci> x </ci>
        </bvar>
        <bvar>
          <ci> y </ci>
        </bvar>
        <piecewise>
          <piece>
            <ci> x </ci>
            <apply>
              <gt/>
              <ci> x </ci>
              <ci> y </ci>
            </apply>
          </piece>
          <otherwise>
            <ci> y </ci>
          </otherwise>
        </piecewise>
      </lambda>
    </math>
  </functionDefinition>
  <functionDefinition id="bounds__min" name="maximum of arguments">
